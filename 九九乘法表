def printMultiTable2() {
  var i = 1 
  while (i <= 9) {
    var j = 1 
    while (j <= i) {
      val prod = (i * j).toString() 
      var k = prod.length() 
      while (k < 4) {
        print(" ")
        k += 1
      }
      print(j + "*" + i + "=" + prod)
      j += 1
    }
    println()
    i += 1
  }
}
